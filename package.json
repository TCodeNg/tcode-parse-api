{
  "name": "tcode-parse",
  "version": "2.0.9",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-redis-cache": "1.1.3",
    "express-winston": "4.0.5",
    "parse-server": "4.10.15",
    "redis": "3.0.2",
    "redis-rejson": "1.0.0",
    "uuid": "8.3.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@kevinpollet/tsconfig": "0.1.0",
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.7",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.8",
    "@types/express-redis-cache": "1.1.0",
    "@types/jest": "26.0.13",
    "@types/node": "14.10.0",
    "@types/parse": "2.12.5",
    "@types/redis": "2.8.27",
    "concurrently": "5.3.0",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "10.3.0",
    "nodemon": "2.0.4",
    "prettier": "2.1.1",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "tsc-watch": "4.2.9",
    "typescript": "4.0.2"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "node 'dist/server.js'",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess 'node dist/server.js'",
    "format": "prettier --write '*.{yml,json,md}' '{src,test}/**/*'",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest --preset ts-jest",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "prestart:dev": "npm run clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,yml,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
